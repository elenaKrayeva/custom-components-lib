{
  "name": "@alena.k/custom-components-lib",
  "version": "0.1.5",
  "description": "React UI components library (TypeScript, Webpack, CSS Modules).",
  "license": "MIT",
  "author": "AlenaK",
  "homepage": "https://github.com/elenaKrayeva/custom-components-lib",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elenaKrayeva/custom-components-lib.git"
  },
  "bugs": {
    "url": "https://github.com/elenaKrayeva/custom-components-lib/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./dist/styles.css": "./dist/styles.css"
  },
  "style": "./dist/styles.css",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
    "@storybook/react-webpack5": "^9.1.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.3",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-util": "^29.7.0",
    "lint-staged": "^16.1.5",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "storybook": "^9.1.3",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "library",
    "typescript",
    "webpack",
    "css-modules"
  ]
}
